{
  "name": "wongames-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src --max-warnings=0",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "schema:download": "apollo client:download-schema --endpoint=http://localhost:1337/graphql schema-temp.gql",
    "schema:convert": "node convertToNonNull.js",
    "codegen": "apollo codegen:generate --target typescript --excludes=node_modules/ --includes=src/graphql/**/*.ts --localSchemaFile schema.gql --tagName=gql --outputFlat src/graphql/generated",
    "types:generate": "yarn schema:download && yarn schema:convert && yarn codegen",
    "storybook": "start-storybook -p 6006 -s ./public",
    "build-storybook": "build-storybook -s ./public",
    "generate": "yarn plop --plopfile ./generators/plopfile.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*": [
      "yarn lint --fix"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.3.21",
    "@styled-icons/boxicons-regular": "^10.23.0",
    "@styled-icons/boxicons-solid": "^10.34.0",
    "@styled-icons/fa-brands": "^10.18.0",
    "@styled-icons/material-outlined": "^10.18.0",
    "@styled-icons/remix-fill": "^10.18.0",
    "graphql": "^15.5.1",
    "joi": "^17.4.2",
    "lodash.xor": "^4.5.0",
    "next": "10.0.6",
    "next-auth": "^3.29.0",
    "next-pwa": "^5.0.5",
    "nextjs-progressbar": "^0.0.11",
    "polished": "^4.1.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-icons": "^4.1.0",
    "react-slick": "^0.28.0",
    "styled-components": "^5.2.1",
    "styled-media-query": "^2.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/preset-typescript": "^7.12.7",
    "@storybook/addon-actions": "^6.3.4",
    "@storybook/addon-controls": "^6.3.4",
    "@storybook/addon-essentials": "^6.3.4",
    "@storybook/addon-links": "^6.3.4",
    "@storybook/react": "^6.3.4",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/react-hooks": "^7.0.1",
    "@testing-library/user-event": "^13.1.9",
    "@types/jest": "^26.0.20",
    "@types/lodash.xor": "^4.5.6",
    "@types/next-auth": "^3.15.0",
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.1",
    "@types/react-slick": "^0.23.4",
    "@types/styled-components": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "apollo": "^2.33.4",
    "babel-loader": "^8.2.2",
    "eslint": "^7.18.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.3.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "jest-styled-components": "^7.0.3",
    "lint-staged": "^10.5.3",
    "plop": "^2.7.4",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  }
}
